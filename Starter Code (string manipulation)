def LEFT(string,length):   
    return string[0:length]

my_string = LEFT("rIHRiYyORf",2)
print(my_string)

def RIGHT(string,length):
    return string[length:]

my_string1 = RIGHT("rIHRiYyORf",2)
print(my_string1)

def MID (string,start,end):
    return string[start:(start+end):1]

my_string2 = MID("rIHRiYyORf",1,4)
print(my_string2)

def LENGTH(string):
    return len(string)

my_string3 = LENGTH("rIHRiYyORf")
print(my_string3)

def LCASE(string): 
    return string.lower()

my_string4 = LCASE("rIHRiYyORf")
print(my_string4)
    
def UCASE(string):
    return string.upper()

my_string5 = UCASE("rIHRiYyORf")
print(my_string5)

def TO_UPPER(string):
    return string.upper()

my_string6 = TO_UPPER("rIHRiYyORf")
print(my_string6)

def TO_LOWER(string):
    return string.lower()

my_string7 = TO_LOWER("rIHRiYyORf")
print(my_string7)
 
def NUM_TO_STRING(integer):
    return str(integer)

my_string8 = NUM_TO_STRING(25.34)
print(my_string8)

def STRING_TO_NUM(string):
    return float(string)

my_string9 = STRING_TO_NUM("24.83")
print(my_string9)

def INT(real):
    return round(real,0)

my_string10 = INT(90.182)
print(my_string10)

def ASC(char):
    return ord(char)

my_string11 = ASC("Y")
print(my_string11)
  



